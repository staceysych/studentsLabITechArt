{
  "name": "anastasiya-sych",
  "version": "1.1.0",
  "description": "",
  "main": "main.tsx",
  "scripts": {
    "test": "jest",
    "serve": "./node_modules/.bin/webpack serve --hot --open --config webpack.devServer.js",
    "json:server": "json-server --watch db.json --routes ./src/api/routes.json",
    "build-prod": "./node_modules/.bin/webpack --mode production --config webpack.prod.js",
    "build-prod-withMap": "./node_modules/.bin/webpack --mode production --config webpack.prod.js --sourceMap",
    "build-dev": "./node_modules/.bin/webpack --mode development --config webpack.dev.js",
    "lint": "lint-staged",
    "lint-code": "./node_modules/.bin/eslint --fix --cache ./**/*.{js,jsx,ts,tsx,json}",
    "lint-styles": "./node_modules/.bin/stylelint --fix --cache ./**/*.{css,scss,less}",
    "lint-md": "./node_modules/.bin/markdownlint --fix **/*.md --ignore node_modules"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "automock": false,
    "setupFiles": [
      "./src/helpers/setUpTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/staceysych/studentsLabITechArt"
  },
  "keywords": [
    "React",
    "Webpack",
    "ReactJS",
    "Webpack4"
  ],
  "author": "Anastasiya.Sych",
  "license": "MIT",
